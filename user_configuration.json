	"base-devel",
	"chezmoi",
	"firefox",
	"fcitx5",
	"git",
	"kitty",
	"neovim"
    ],
    "parallel downloads": 5,
    "profile_config": {
	"gfx_driver": null,
	"greeter": null,
	"profile": {
	    "custom_settings": {},
	    "details": ["sway"],
	    "main": "Minimal"
	}
    },
    "script": "guided",
    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": true,
    "timezone": "UTC",
    "uki": true,
    "version": "2.8.1"
}
