{
    "additional-repositories": [
	"multilib"
    ],
    "archinstall-language": "English",
    "audio_config": {
	"audio": "pipewire"
    },
    "bootloader": "efistub",
    "config_version": "2.8.1",
    "custom-commands": [
	"git clone https://aur.archlinux.org/paru.git",
	"cd paru",
	"makepkg -si",
	"chezmoi init --apply real-LiHua"
    ],
    "debug": false,
    "disk_config": {
	"config_type": "pre_mounted_config",
	"mountpoint": "."
    },
    "disk_encryption": null,
    "hostname": "localhost",
    "kernels": [
	"linux-zen"
    ],
    "locale_config": {
	"kb_layout": "us",
	"sys_enc": "UTF-8",
	"sys_lang": "zh_CN"
    },
    "mirror_config": {
	"custom_mirrors": [
	    {
		"name": "archlinuxcn",
		"url": "https://repo.archlinuxcn.org/$arch"
	    }
	],
	"mirror_regions": {
	    "China": [
